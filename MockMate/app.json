{
  "expo": {
    "name": "MockMate",
    "slug": "mockmate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/Logo/Logo.png",
    "scheme": "mockmate",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.mockmate.ai"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#F5F6F8",
        "foregroundImage": "./assets/images/Logo/Logo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.mockmate.ai"
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/Logo/favicon-32x32.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/Logo/Logo.png",
          "resizeMode": "cover",
          "backgroundColor": "#F5F6F8"
        }
      ],
      [
        "expo-font",
        {
          "fonts": ["./assets/fonts/Lexend-VariableFont.ttf"]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
