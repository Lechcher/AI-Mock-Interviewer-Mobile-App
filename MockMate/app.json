{
  "expo": {
    "name": "MockMate",
    "slug": "MockMate",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "mockmate",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#FFFFFF",
        "foregroundImage": "@/assets/images/Logo/Logo.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "@/assets/images/Logo/favicon-32x32.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "@/assets/images/Logo/Logo.png",
          "resizeMode": "cover",
          "backgroundColor": "#ffffff"
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            "@/assets/fonts/Lexend-Black.ttf",
            "@/assets/fonts/Lexend-Bold.ttf",
            "@/assets/fonts/Lexend-ExtraBold.ttf",
            "@/assets/fonts/Lexend-ExtraLight.ttf",
            "@/assets/fonts/Lexend-Light.ttf",
            "@/assets/fonts/Lexend-Medium.ttf",
            "@/assets/fonts/Lexend-Regular.ttf",
            "@/assets/fonts/Lexend-SemiBold.ttf",
            "@/assets/fonts/Lexend-Thin.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
